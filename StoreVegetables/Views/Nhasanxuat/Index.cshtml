@model IEnumerable<StoreVegetables.Models.NhaSanXuat>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPAdmin.cshtml";
}

<h2>QUẢN LÝ DANH SÁCH NHÀ XUẤT BẢN</h2>

<p>
    @Html.ActionLink("THÊM MỚI", "Create")
</p>
<table class="table">
    <tr>
        <th>
            TÊN NHÀ XUÂT BẢN
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tenNSX)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.tenNSX }) 

        </td>
    </tr>
}

</table>
